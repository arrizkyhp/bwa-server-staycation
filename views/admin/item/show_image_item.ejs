<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">
    <div class="row">
        <% item.imageId.map( (e) => { %>
        <div class="col-md-4">
            <div class="card shadow mb-4 mt-4">
                <div class="card-body">
                    <img src="https://admin-staycation-arrizky.herokuapp.com/<%= e.imageUrl %>" alt="" width="100%">
                </div>
            </div>
        </div>
        <% }) %>
        <a href="/admin/item" class="btn btn-warning ml-2">Back</a>
    </div>
</div>